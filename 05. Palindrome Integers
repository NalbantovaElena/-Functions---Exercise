function solve(input) {

    input.forEach(element => {
        let str = ''
        element = element.toString()
        for (let i = element.length - 1; i >= 0; i--) {
            //console.log(element[i])
            str += element[i]
        }
        //console.log(str)
        if (str === element) {
            console.log(true)
        } else {
            console.log(false)
        }
    });
}
